{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }
    ],
    "redirects": [
      {
        "source": "/:path*",
        "destination": "/",
        "permanent": false,
        "has": [
          {
            "type": "header",
            "key": "x-vercel-deployment",
            "value": "true"
          }
        ]
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
  